{%- assign font-family-base = settings.font-family-base -%}
{%- assign font-family-second = settings.font-family-second -%}
{%- assign font-family-base_bold = font-family-base | font_modify: 'weight', 'bolder' -%}
{%- assign font-family-base_italic = font-family-base | font_modify: 'style', 'italic' -%}
{%- assign font-family-base_bold_italic = font-family-base_bold | font_modify: 'style', 'italic' -%}
{%- assign font-family-second_bold = font-family-second | font_modify: 'weight', 'bolder' -%}
{%- assign font-family-second_medium = font-family-second | font_modify: 'weight', '500' -%}

$blue: #007bff;
$indigo: #6610f2;
$purple: #6f42c1;
$pink: #e83e8c;
$red: #f00;
$orange: #fd7e14;
$yellow: #ffc107;
$green: #28a745;
$teal: #20c997;
$cyan: #17a2b8;
$white: #fff;
$black: #000;
$gray: #6c757d;
$gray-dark: #343a40;
$nocolor: transparent;
$shadow: 0px 6px 15px rgba(0, 0, 0, .12);

$theme-color: {{ settings.theme-color }};
$secondary: {{ settings.color_secondary }};
$text-color: {{ settings.text-color }};
$gray-dark: {{ settings.gray-dark }};
$body-bg: {{ settings.color_body_bg }};
$border-color: {{ settings.color_border }};

{% if settings.select-font-base == 'google' and settings.font-family-base-google != blank %}
	$font-family-base: {{ settings.font-family-base-google }};
{% else %}
	$font-family-base: {{ font-family-base.family }}, {{ font-family-base.fallback_families }};
{% endif %}
$body-font-weight: {{ font-family-base.weight }};
$body-font-weight-bold: {{ font-family-base_bold.weight | default: 600 }};
$body-font-style: {{ font-family-base.style }};
$font-size-base: {{ settings.font-size-base | append: 'px' }};

{% if settings.select-font-second == 'google' and settings.font-family-second-google != blank %}
	$font-family-second: {{ settings.font-family-second-google }};
{% else %}
	$font-family-second: {{ font-family-second.family }}, {{ font-family-second.fallback_families }};
{% endif %}
$heading-font-weight: {{ font-family-second.weight }};
$heading-font-weight-bold: {{ font-family-second_bold.weight | default: 600 }};
$heading-font-weight-medium: {{ font-family-second_medium.weight | default: 500 }};
$heading-font-style: {{ font-family-second.style }};
$heading-font-size: {{ settings.type_header_base_size | append: 'px' }};

$font-family: $font-family-base;
$font-size: $font-size-base;
$text-color: $text-color;

$font-icon: 'FontAwesome';
$gutter-width: 30px;


.prev_next_buttons_product{
	position:fixed;
	top:30%;
	left:20px;
	z-index:9999;
	transition:all 0.3s ease;
	-webkit-transition:all 0.3s ease;
	opacity:0;
	visibility:hidden;
	transform:translateX(-100%);
	-webkit-transform:translateX(-100%);
	&.active{
		opacity:1;
		visibility:visible;
		transform:translateX(0);
		-webkit-transform:translateX(0);
	}
	.image{
		position:absolute;
		opacity:0;
		visibility:hidden;
		width:180px;
		bottom:calc(50% - 60px);
		z-index:50000;
		left:calc(100% + 20px);
		-webkit-transition:all 0.5s ease;
		transition:all 0.5s ease;
		height:120px;
		.title{
			font-size:15px;
			color:$white;
			position:absolute;
			bottom:0;
			margin:0;
			padding:15px;
			width:100%;
			background: -webkit-linear-gradient(bottom, rgba($gray-dark,0.5) 0%,rgba(255,255,255,0) 100%); /* Chrome10-25,Safari5.1-6 */
			background: linear-gradient(to top, rgba($gray-dark,0.5) 0%,rgba(255,255,255,0) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
		}
		img{
			width:100%;
			object-fit: cover;
			height: 100%;
		}
	}
	>div:not(.continue-shop){
		position:relative;
		height:50px;
		line-height:50px;
		justify-content:center;
		font-size:0;
		transition:all 0.5s ease;
		>a{
			display:inline-block;
			position:relative;
			font-size:25px;
		}
		&:hover{
			.image{
				opacity:1;
				visibility:visible;
				left:calc(100% + 5px);
			}
		}
	}
	.continue-shop{
		font-size:25px;
		height:50px;
		line-height:50px;
		position:relative;
		span{
			font-size:16px;
			color:$gray-dark;
			position:absolute;
			white-space:nowrap;
			left:calc(100% + 20px);
			opacity:0;
			visibility:hidden;
			transition:all 0.5s ease;
			top:-2px;
		}
		&:hover{
			span{
				opacity:1;
				visibility:visible;
				left:calc(100% + 5px);
			}
		}
	}
	@media (max-width:1500px){
		left:0;
	}
	@media (max-width:1199px){
		width:100%;
		display:flex;
		padding:0 15px;
		justify-content:space-between;
		position:unset;
		top:0;
		margin: 40px 0 -40px;
		.continue-shop{
			order:2;
		}
		.next_button{
			order:3;
		}
		.image{
			display:none;
		}
	}
}
.sticky-cart-single{
	position: fixed;
    width: 100%;
    bottom: 0;
    background:$white;
    z-index: 9;
    padding: 15px 0;
    box-shadow: 3px 3px 57px 0 rgba($gray-dark,0.05);
    -webkit-animation-name: sticky-product;
    animation-name: sticky-product;
    -webkit-animation-duration: .5s;
    animation-duration: .5s;
    -webkit-animation-timing-function: ease-in-out;
    animation-timing-function: ease-in-out;
    -webkit-animation-iteration-count: 1;
    animation-iteration-count: 1;
    left: 0;
	display:none;
	&.sticky{
		display:block;
		@media (max-width:767px){
			display:none;
		}
	}
	.content{
		max-width: 1440px;
		padding: 0 15px;
		margin-left: auto;
		margin-right: auto;
		display: flex;
		justify-content: space-between;
		align-items: center;
	}
	.group-option-selector{
		color:$gray-dark;
		font-weight:600;
		font-size:13px;
		margin-top: 2px;
		span{
			&:after{
				content:"/";
				display:inline-block;
				margin:0 5px;
			}
			&:last-child{
				&:after{
					display:none;
				}
			}
		}
	}
	.content-option{
		.single-option-selector{
			&:last-child{
				display:none;
			}
			&:only-child{
				display:block;
			}
		}
	}
	.content-product{
		display: flex;
		align-items: center;
		.content-bottom{
			padding-left: 15px;
		}
		.item-title{
			font-size: 14px;
			margin-top: -5px;
			color:$gray-dark;
		}
		.price-container{
			color:$theme-color;
			font-size:15px;
			.product-single__unit{
				display:none;
			}
			.product-single__price--wrapper{
				font-size: 14px;
				color: #9b9b9b;
				text-decoration: line-through;
				margin-right:5px;
			}
		}
	}
	.item-thumb{
		img{
			max-width: 85px;
		}
	}
	.product-form__input{
		border: 0;
		min-width: 50px;
		height: 40px;
		padding: 0 10px;
		font-size:15px;
		width:100%;
		border:1px solid #e5e5e5;
		&:focus-visible {
			outline: unset;
		}
		option{
			line-height:30px;
			font-size:14px;
		}
	}
	.single-option-selector {
		position: relative;
		margin: 0 10px 10px 0;
		input {
			clip: rect(0, 0, 0, 0);
			overflow: hidden;
			position: absolute;
			width: 1px;
			height: 1px;
		}
		label {
			cursor: pointer;
			position: relative;
			display: block;
			margin: 0;
			padding: 9px 15px;
			background-color: $white;
			border: 1px solid #ccc;
			font-size: 16px;
			line-height: 1;
			text-align: center;
			color:$text-color;
			font-family:$font-family-second;
			text-align: center;
			line-height: 20px;
			padding: 14px 10px 12px;
			min-width: 50px;
			&[data-color] {
				width: 34px;
				height: 34px;
				padding: 0;
				font-size: 0;
				text-indent: -9999;
				background-repeat: no-repeat;
				background-position: center;
				background-size: 100%;
				&.disabled {
					&::before {
						content: '';
						pointer-events: none;
						position: absolute;
						top: 0;
						left: 0;
						width: 100%;
						height: 100%;
						background-image: url({{ 'ico-cross.svg' | asset_url }});
						background-repeat: no-repeat;
						background-position: center;
						background-size: 100%;
					}
				}
			}
			&.disabled {
				cursor: default;
				color: darken($border-color, 20%) !important;
				background-color: lighten($border-color, 4%) !important;
			}
		}
		input[type='radio']:checked {
			+ label {
				border-color: $gray-dark;
				background:$gray-dark;
				color:$white;
				&[data-color] {
					box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16), 0 2px 10px 0 rgba(0,0,0,0.12);
				}
			}
		}
	}
	.product-quantity__select {
		border: 0;
		min-width: 50px;
		height: 45px;
		padding: 0 10px;
		font-size:15px;
		&:focus-visible {
			outline: unset;
		}
		option{
			line-height:30px;
			font-size:14px;
		}
	}
	.product-single__external{
		-webkit-transition: none;
		transition: none;
		font-size: 12px;
		text-transform: uppercase;
		line-height: 40px;
		height:40px;
		position: relative;
		background: $gray-dark;
		color:$white;
		text-align: center;
		padding: 0 25px;
		cursor: pointer;
		display: inline-block;
		margin: 0;
		white-space: nowrap;
		font-family:$font-family-base;
		width:auto;
		margin-right:10px;
		&:hover{
			background:$theme-color;
		}
	}
	.product-single__buttons {
		display:flex;
		flex-wrap:wrap;
		.btn--add-to-cart{
			-webkit-transition: none;
			transition: none;
			font-size: 12px;
			text-transform: uppercase;
			line-height: 36px;
			height:40px;
			position: relative;
			background: transparent;
			border:2px solid $gray-dark;
			color: $gray-dark;
			text-align: center;
			padding: 0 25px;
			cursor: pointer;
			display: inline-block;
			margin: 5px 0;
			white-space: nowrap;
			font-family:$font-family-base;
			width:auto;
			margin-right:10px;
			&:hover{
				background:$theme-color;
				border-color:$theme-color;
				color:$white;
			}
		}
		.shopify-payment-button{
			.shopify-payment-button__button{
				background:$theme-color;
				border-radius:0;
				font-size: 12px;
				text-transform: uppercase;
				line-height: 40px;
				height:40px;
				position: relative;
				padding: 0 25px;
				cursor: pointer;
				display: inline-block;
				white-space: nowrap;
				font-weight:400;
				margin:5px 0;
				&:hover{
					background:$gray-dark;
				}
			}
			.shopify-payment-button__button--hidden{
				display:none;
			}
		}
		.product-quantity{
			margin: 5px 10px 5px 0;
			border: 2px solid #b5b5b5;
			height: 40px;
			display: inline-block;
			position: relative;
			.wpbingo-qty__number{
				width: 100px;
				border: 0;
				height: 36px;
				text-align: center;
				padding: 0 25px;
				font-size: 18px;
				font-weight: 500;
			}
			.wpbingo-qty__adjust--minus{
				position: absolute;
				top: 0;
				left: 0;
				background: transparent;
				line-height: 24px;
				border: none;
				width: 25px;
				height: 36px;
				color:$text-color;
				font-size:16px;
				padding:0;
				&:hover{
					color:$theme-color;
				}
			}
			.wpbingo-qty__adjust--plus{
				position: absolute;
				top: 0;
				right: 0;
				background: transparent;
				line-height: 24px;
				border: none;
				font-size:16px;
				width: 25px;
				height: 36px;
				padding:0;
				color:$text-color;
				&:hover{
					color:$theme-color;
				}
			}
		}
	}
}
.product-single{
	margin-top:50px;
	>.row{
		>div {
			&.bwp-single-info {
				padding-left:80px;
				.wpbingo-breadcrumbs{
					margin-top: 0px;
				}
				@media (max-width:991px){
					padding-left:15px;
				}
			}
		}
	}
	.inventory-content{
		margin-bottom:5px;
	}
	.product-single__short-description{
		margin-bottom:25px;
	}
	.countdown-single{
		margin-bottom: 30px;
		h2{
			margin: 0 0 20px;
			font-size: 22px;
			position: relative;
			color: #f55;
			font-weight: 500;
			i{
				font-size:24px;
				margin-right:10px;
			}
		}
		.countdown{
			padding:0;
			margin:0;
			justify-content:flex-start;
			.countdown__item{
				margin:0 17px 0 0;
				>span{
					&:first-child{
						background:transparent;
						width: 66px;
						height: 66px;
						border-radius: 50%;
						-webkit-border-radius: 50%;
						-moz-border-radius: 50%;
						-ms-border-radius: 50%;
						-o-border-radius: 50%;
						border: 2px solid #ff4848;
						text-align: center;
						line-height: 62px;
						font-size: 25px;
						color: $gray-dark;
						display: inline-block;
						font-family:$font-family-base;
					}
					&:last-child{
						display: block;
						font-size: 14px;
						color: $gray-dark;
						font-weight: 500;
						margin-top: 0;
						text-transform:unset;
						opacity:1;
					}
				}
				@media (max-width:480px){
					margin:0 10px 0 0;
					>span{
						&:first-child{
							background:transparent;
							width: 55px;
							height: 55px;
							line-height: 53px;
							font-size: 18px;
						}
					}
				}
			}
		}
	}
	.size-guide{
		margin-bottom: 25px;
		&:before{
			position:fixed;
			content:"";
			height:100vh;
			width:100vw;
			background:rgba($gray-dark,0.5);
			top:50%;
			left:50%;
			transform:translate(-50%,-50%);
			z-index:99999;
			display:none;
		}
		>.title-size-guide{
			font-weight: 700;
			text-transform: capitalize;
			color: $gray-dark;
			margin: 0;
			display:inline-flex;
			cursor:pointer;
			z-index:10;
			transition:all .3s ease;
			h2{
				font-size:16px;
				margin:0;
				text-decoration: underline;
				font-weight:600;
			}
			i{
				margin-right:10px;
				font-size:22px;
				position:relative;
				top:-2px;
			}
			&:hover{
				color:$theme-color;
				h2{
					color:$theme-color;
				}
			}
		}
		.img-size-guide{
			position:fixed;
			top:50%;
			left:50%;
			transform:translate(-50%,-50%);
			z-index:99999;
			max-height:70vh;
			overflow:auto;
			display:none;
			@media (max-width:1199px){
				max-height:90vh;
			}
			@media (max-width:991px){
				max-width:80vw;
				width:100%;
			}
			@media (max-width:480px){
				max-width:100%;
				width:100%;
			}
			&::-webkit-scrollbar-track
			{
				background-color: #F5F5F5;
			}

			&::-webkit-scrollbar
			{
				width: 5px;
				background-color: #F5F5F5;
			}

			&::-webkit-scrollbar-thumb
			{
				background-color: rgba($gray-dark, 0.1);
			}
			.title-size-guide{
				position:absolute;
				top:10px;
				right:10px;
				color:$gray-dark;
				width:30px;
				height:30px;
				border:1px solid #e1e1e1;
				line-height:30px;
				text-align:center;
				border-radius:50%;
				font-size:20px;
				cursor:pointer;
				.icon_close{
					transition:all .3s ease;
					display: inline-block;
				}
				&:hover{
					background: $theme-color;
					border-color: $theme-color;
					.icon_close{
						transform:rotate(90deg);
						color: $white;
					}
				}
			}
		}
		&.active{
			&:before{
				display:block;
			}
			.img-size-guide{
				display:block;
			}
		}
	}
	.content-form{
		display: inline-block;
		width: 100%;
		border-bottom: 1px solid #e5e5e5;
		padding-bottom: 30px;
		margin-bottom: 30px;
		.js-product-thumbnails{
			border-top: 1px solid #e5e5e5;
			padding-top:30px;
			margin-top:30px;
			.slick-list{
				margin:0 -7.5px;
			}
			.product-single__thumbnail-wrapper{
				padding:0 7.5px;
			}
			.slick-arrow{
				width:30px;
				height:30px;
				line-height:28px;
				margin-top:0;
				opacity:0;
				visibility:hidden;
				background:$theme-color;
				border-color:$theme-color;
				color:$white;
				top:50%;
				&:before{
					font-size:15px;
				}
				&.slick-prev {
					left:-15px;
				}
				&.slick-next{
					right:-15px;
				}
				&.slick-disabled{
					cursor:not-allowed;
					opacity:0.3 !important;
				}
				&:hover{
					background:$gray-dark;
				}
				@media (max-width:991px){
					width:20px;
					height:20px;
					border-radius:50%;
					line-height:18px;
					opacity:1;
					visibility:visible;
				}
			}
			@media (max-width:767px){
				.slick-list{
					margin:0 -5px;
				}
				.product-single__thumbnail-wrapper{
					padding:0 5px;
				}
			}
			&:hover{
				.slick-arrow{
					opacity:1;
					visibility:visible;
				}
			}
		}
	}
	.product__organization{
		display: block;
		width: 100%;
		margin-bottom: 10px;
		.product__organization__label{
			color: $text-color;
			display:inline-block;
			margin-right:10px;
		}
		.product__organization__text,a,span{
			color:$gray-dark;
			display:inline-block;
		}
		a:hover{
			color:$theme-color;
		}
		.social-sharing{
			display:inline-block;
			float: unset;
			width: auto;
			.share-title{
				display:none;
			}
			a:hover span{
				color:$theme-color;
			}
		}
	}
	.product-single__title{
		font-size:40px;
		margin:10px 0 20px;
		@media (max-width:767px){
			font-size:30px;
		}
	}
	.price-container{
		color:$theme-color;
		font-size:22px;
		width: 100%;
		margin-bottom: 30px;
		padding-bottom: 30px;
		border-bottom: 1px solid #e5e5e5;
		line-height: 100%;
		display: flex;
		.product-single__unit{
			display:none;
		}
		.product-single__price--wrapper{
			font-size: 18px;
			color: #9b9b9b;
			text-decoration: line-through;
			margin-right:10px;
		}
	}
	.group_table{
		border: 1px solid $border-color;
		margin-bottom: 30px;
		width: 100%;
		.item-product-group{
			display: flex;
			padding: 12px 15px;
			border-bottom: 1px solid $border-color;
			justify-content: space-between;
			align-items: center;
			flex-wrap: wrap;
			&:last-child{
				border-bottom:0;
			}
			.product-content{
				display: flex;
				align-items: center;
				flex: 1;
			}
			.product-thumb{
				flex: 0 0 60px;
				margin-right: 10px;
			}
			.product-title{
				font-size: 14px;
				font-weight: 400;
				margin: 0 0 2px;
				-webkit-line-clamp: 2;
				-webkit-box-orient: vertical;
				overflow: hidden;
				max-width: 250px;
				font-family:$font-family-base;
				display: -webkit-box;
				a{
					color:$text-color;
					&:hover{
						color:$theme-color;
					}
